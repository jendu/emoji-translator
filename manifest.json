{
  "name":"Emoji Translator",
  "version":"1.0",
  "description":"Translates emotes into their respective descriptions.",
  "author":"Jennifer Du",
  "manifest_version":2,

  "permissions":[
    "declarativeContent",
    "storage",
    "activeTab"
  ],
  "background":{
    "scripts":["bg.js"],
    "persistent":false
  },
  "page_action":{
    "default_popup":"popup.html",
    "default_icon":{
      "16":"images/logo16.png",
      "32":"images/logo32.png",
      "48":"images/logo48.png",
      "128":"images/logo128.png"
    }
  }
}
